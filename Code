// App.js


import React from 'react';
import {
  Button,
  SafeAreaView,
  Text,
  View,
} from 'react-native';
import { Colors } from 'react-native/Libraries/NewAppScreen';

const App = () => {
  return(
    <SafeAreaView style={{backgroundColor: 'pink', height: '100%'}}>
      <View>
        <Text style={textStyle}>I'm Now a React Developer</Text>
      </View>

      <View style={{width:"25%", marginLeft:"38%"}}>
      <Button 
      title = "CLICK ME"
      />
      </View>

      <View>
        <Text style={ADVStyle}>ADV101</Text>
      </View>
    </SafeAreaView>
  );
}
const textStyle = {
  color: 'green',
  textAlign: 'center',
  marginTop: 350,
  fontWeight: 600,
  fontSize: 25
}
const ADVStyle = {
  fontWeight: 600, 
  textAlign:'right', 
  marginTop:"80%",
  fontSize: 50,
  color: 'red'
}
export default App;




// TERMINAL


